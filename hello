import java.util.Scanner;
class Hello
{
public static void main(String args[])
{
int rows;
char n='A';
System.out.println("enter no.of rows");
Scanner sc =new Scanner(System.in);
rows=sc.nextInt();
for(int i=1;i<=rows
;i++)
{            
for(int j=1;j<=i;j++)
{
System.out.print((char)n+" ");
n++;
}
System.out.println();
}
}
}
